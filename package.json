{
  "name": "ampel",
  "version": "1.0.0",
  "description": "Ampel is a lightweight, dependency free load balancer that is tailored to a one specific use case. Enjoy.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "nodemon src/server.js --exec babel-node",
    "start:debug": "nodemon src/server.js --exec 'babel-node --inspect=5858'",
    "build": "babel src -d dist"
  },
  "author": "Nir Azuelos <nirazuelos@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^ 2.0.16",
    "regenerator-runtime": "^0.13.9"
  }
}
